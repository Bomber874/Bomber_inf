#include <fstream> 
#include <iostream> 
#include <cstring>

using namespace std;
void lab1() {
    ifstream file("input.txt");
    int numbers[20];
    for (int i = 0; i < 20; i++) {
        file >> numbers[i];
    }
    file.close();

    ofstream fin("output.txt");
    for (int i = 0; i < 20; i++) {
        fin << numbers[i] + numbers[19] << endl;
    }
    fin.close();
}

struct student
{
    string surname;
    string group;
    short balls[5];
};

